{
    "name": "@finos/perspective-workspace",
    "version": "1.0.1",
    "description": "Perspective Workspace",
    "main": "./dist/cjs/perspective-workspace.js",
    "module": "./dist/cjs/perspective-workspace.js",
    "browser": "dist/cjs/perspective-workspace.js",
    "files": [
        "dist/**/*",
        "index.d.ts"
    ],
    "unpkg": "./dist/umd/perspective-workspace.js",
    "jsdelivr": "./dist/umd/perspective-workspace.js",
    "scripts": {
        "bench": "yarn npm-run-all bench:build bench:run",
        "bench:build": "echo \"No Benchmarks\"",
        "bench:run": "echo \"No Benchmarks\"",
        "build:babel": "yarn cpy \"src/**/*\" dist && yarn babel src/js --source-maps --out-dir dist/esm",
        "build:styles": "yarn webpack --color --config config/theme.config.js",
        "build:webpack:cjs": "yarn webpack --color --config config/cjs.config.js",
        "build:webpack:umd": "yarn webpack --color --config config/umd.config.js",
        "build:webpack": "yarn npm-run-all build:webpack:cjs build:webpack:umd",
        "build": "yarn npm-run-all --silent build:babel build:webpack:cjs build:webpack:umd build:styles",
        "clean": "yarn rimraf dist",
        "clean:screenshots": "yarn rimraf \"test/screenshots/**/*.@(failed|diff).png\"",
        "test:run": "yarn jest --rootDir=. --config=../perspective-test/jest.config.js --no-stacktrace --color 2>&1",
        "test:unit": "yarn jest  --color --config=./config/jest.unit.config.js 2>&1",
        "test:integration": "yarn jest --color  --config=./config/jest.integration.config.js 2>&1",
        "watch": "PSP_DEBUG=1 yarn webpack --color --watch --config config/watch.config.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/finos/perspective"
    },
    "author": "",
    "license": "Apache-2.0",
    "dependencies": {
        "@finos/perspective-viewer": "^1.0.1",
        "@lumino/algorithm": "1.3.3",
        "@lumino/commands": "1.12.0",
        "@lumino/domutils": "1.2.3",
        "@lumino/messaging": "1.4.3",
        "@lumino/virtualdom": "1.8.0",
        "@lumino/widgets": "1.17.0",
        "lodash": "^4.17.4"
    },
    "devDependencies": {
        "@babel/cli": "npm:^7.8.4",
        "@finos/perspective-test": "^1.0.1",
        "cpy-cli": "npm:^3.1.1",
        "jest": "npm:^25.1.0",
        "npm-run-all": "npm:^4.1.3",
        "rimraf": "npm:3.0.2",
        "webpack": "workspace:^"
    }
}
